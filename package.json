{
	"name": "template-worker-router",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy index.js",
		"dev": "wrangler dev index.js --remote",
		"init-dev": "wrangler r2 bucket create ai-images",
		"init-db": "wrangler d1 create openai-cf && wrangler d1 execute openai-cf --file=./schema.sql",
		"init-prod": "wrangler r2 bucket create openai-cf",
		"start-stackblitz": "WRANGLER_SEND_METRICS=false wrangler dev index.js --local",
		"test": "vitest"
	},
	"dependencies": {
		"@cloudflare/ai": "^1.0.15",
		"itty-router": "^2.6.1"
	},
	"devDependencies": {
		"vitest": "^0.31.0",
		"wrangler": "^3.0.0"
	}
}
